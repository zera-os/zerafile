{
  "name": "@zerafile/shared",
  "version": "1.0.0",
  "description": "Shared utilities for zerafile",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./ids": {
      "types": "./dist/ids.d.ts",
      "import": "./dist/ids.js",
      "require": "./dist/ids.js"
    },
    "./mime": {
      "types": "./dist/mime.d.ts",
      "import": "./dist/mime.js",
      "require": "./dist/mime.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
